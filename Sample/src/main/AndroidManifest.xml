<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="org.notlocalhost.superlistview.sample" >

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="org.notlocalhost.superlistview.sample.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="org.notlocalhost.superlistview.sample.GridSample"
            android:label="@string/title_activity_grid_sample" >
        </activity>
        <activity
            android:name="org.notlocalhost.superlistview.sample.ListSample"
            android:label="@string/title_activity_list_sample" >
        </activity>
    </application>

</manifest>
